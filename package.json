  "devDependencies": {
    "jasmine": "^3.8.0",
    "jasmine-core": "^3.8.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-spec-reporter": "^0.0.32",
    "respec": "^26.9.1"
